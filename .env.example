# Server
NODE_ENV=development
PORT=3000
BASE_URL=http://localhost:3000

# Database
DB_PATH=./data/square.db

# Admin credentials (in production use strong values)
ADMIN_USER=admin
ADMIN_PASS=change_me_in_production

# Ingest API token
INGEST_TOKEN=your_secret_token_here

# Uploads
UPLOADS_DIR=./uploads
MAX_FILE_SIZE=10485760

# Limits
MAX_TEXT_LENGTH=50000
RATE_LIMIT_MAX=100
RATE_LIMIT_TIMEWINDOW=60000

# RSS
RSS_FEED_SIZE=50
RSS_CACHE_TTL=300000
